{
  "manifest_version": 2,
  
  "name": "AutomateAutomation",
  "version": "0.0.1",
  "description": "Automate Automation",
  "homepage_url": "http://anilpradhan.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },

  "browser_action": {
    "default_title": "AutomateAutomation",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "contextMenus"
  ]
}